[ print new line ]
print.new_line()() = {
    frost.set(frost.integer.10)(character)
    frost.print.character(character)()
}

[ entry point ]
main()() = {
    [ setup file paths ]
    frost.set("json/parser_1.json")(blueprint.start blueprint.end)

    [ print file path ]
    frost.print.buffer_as_string(blueprint.start blueprint.end)()

    [ print new line for next program ]
    print.new_line()()
}
